<div id="data-step1" class="product-slider-main position-r" data-temp="tabdata">
  <div class="owl-carousel pro_cat_slider">
  <tal:rep repeat="product view/productBrains">
  <tal:def define="obj product/getObject">
    <div class="item">
      <div class="product-item">
        <div class="sale-label"><span>Sale</span></div>
        <div class="product-image"> <a href="${python: obj.absolute_url()}"> 
		<img src="${python: obj.absolute_url()}/@@images/cover" style="width:278px;height:327.5px"> </a>
          <div class="product-detail-inner">
	    <div class="detail-inner-left left-side">
	    </div>
          </div>
        </div>
        <div class="product-item-details">
          <div class="product-item-name"> <a href="${python: obj.absolute_url()}">${python: obj.modelNo}</a> </div>
          <div class="price-box"> <span class="price">${python: obj.title}</span>  </div>
        </div>
      </div>
    </div>
  </tal:def>
  </tal:rep>
  </div>
</div>
